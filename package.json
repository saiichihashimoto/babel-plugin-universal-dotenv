{
  "name": "babel-plugin-universal-dotenv",
  "version": "0.0.0-development",
  "description": "create-react-app's dotenv resolution as a babel plugin",
  "keywords": [
    "babel",
    "babel-plugin",
    "create-react-app",
    "dotenv",
    "universal-dotenv"
  ],
  "homepage": "https://saiichihashimoto.github.io/babel-plugin-universal-dotenv/",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/babel-plugin-universal-dotenv/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saiichihashimoto/babel-plugin-universal-dotenv.git"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --no-babelrc --source-maps --delete-dir-on-start --verbose --ignore **/*.spec.js",
    "test": "if [ -z \"$CI\" ]; then jest --watch; else jest --color --coverage; fi"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint-config-saiichihashimoto": "1.0.5",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-my-app": "3.1.3"
  }
}
